{
  "$schema": "https://json.schemastore.org/prettierrc.json",
  "arrowParens": "always",
  "bracketSameLine": true,
  "endOfLine": "lf",
  "jsxSingleQuote": true,

  "plugins": ["prettier-plugin-pkg", "prettier-plugin-jsdoc"],
  "printWidth": 80,
  "quoteProps": "consistent",
  "semi": true,
  "tabWidth": 2
}
